{
	"manifest_version": 3,
	"name": "Wikiscript",
	"version": "1.0",
	"description": "Marks visited Wikipedia topics in multiple languages.",
	"permissions": ["history"],
	"action": {
		"default_popup": "index.html"
	},
	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	}
}